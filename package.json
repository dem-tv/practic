{
  "name": "rdm-reporter-front",
  "version": "0.0.1",
  "scripts": {
    "start": "docker-compose up -d && docker-compose exec nodejs npm run dev",
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
    "build:dev": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
    "build:test": "vue-tsc --noEmit --skipLibCheck && vite build --mode testing",
    "build:stage": "vue-tsc --noEmit --skipLibCheck && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
    "preview": "vite preview",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore",
    "typecheck": "vue-tsc --noEmit --skipLibCheck -p tsconfig.json --composite false",
    "test": "vitest"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/core": "^13.3.0",
    "dayjs": "^1.11.13",
    "perfect-scrollbar": "^1.5.6",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "tippy": "^0.0.0",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@types/node": "^22.15.30",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import-x": "^4.15.1",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "stylelint": "^16.20.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.34.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-stylelint": "^6.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.2.3",
    "vue-component-type-helpers": "^2.2.10",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=22.16.0"
  }
}
